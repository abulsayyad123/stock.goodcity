{{#init-foundation}}
  {{#unless isMobileApp}}
    {{smart-banner
      appIdAndroid = app_id
      appIdIOS = ios_app_id
      showAfterClose=3
      showAfterVisit=3
      iconUrl = bannerImage
      title = appTitle
      description = 'Crossroads'
    }}
  {{/unless}}
  {{outlet "notifications"}}
{{/init-foundation}}
{{outlet}}
<div class="printer_message_block"></div>
